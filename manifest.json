{
    "name": "FB Timer",
    "description": "Timer for Facebook",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup" : "hello.html", //  extensions window
        "default_icon" : "hello_extensions.png" //  extensions icon
    },
    "permissions": ["tabs", "background", "activeTab" ],
    "background": {
        "scripts" : ["background.js"],
        "persistent": true
    }

}